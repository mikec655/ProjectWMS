{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=FoodShare;Integrated Security=SSPI;"
  },
  "Kestrel": {
    "Endpoints": {
      "HTTPS": {
        "Url": "https://*:51768"
      }
    }
  },
  "pwa": {
    "cacheId": "Worker 1.1",
    "strategy": "cacheFirstSafe",
    "routesToPreCache": "/, /login",
    "offlineRoute": "index.html",
    "registerServiceWorker": true,
    "registerWebmanifest": true
  }
}
